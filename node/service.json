{
  "memory": 256,
  "ttl": 10,
  "timeout": 5,
  "minReplicas": 2,
  "maxReplicas": 4,
  "routes": {
    "setup": {
      "path": "/seller/setup",
      "settingsType": "workspace"
    },
    "customSetup": {
      "path": "/seller/setup/custom"
    },
    "requests": {
      "path": "/requests"
    },
    "channels": {
      "path": "/seller/channels(/:vendor)",
      "settingsType": "workspace"
    },
    "handshake": {
      "path": "/marketplace/handshake"
    },
    "acceptRequest": {
      "path": "/marketplace/requests/accept"
    },
    "acceptNotification": {
      "path": "/seller/requests/accept"
    }
  }
}
